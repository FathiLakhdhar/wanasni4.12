{% extends ':Trajet:layout.html.twig' %}


{% block title %} Resultat-Rechercher {% endblock title %}

{% block body %}


    <section class="container section-padding-top-bottom-80">
        {{ form_start(form) }}

        {{ form_end(form) }}




        <ul>
            {% for trajet in trajets %}
                <li>
                    {{ trajet.id }} : {{ trajet.origine.lieu }} ==> {{ trajet.destination.lieu }}
                    <ul>
                        <li> {{ trajet.datesAllerToArray|join('|') }}  </li>
                        <li> {{ trajet.datesRetourToArray|join('|') }}  </li>
                    </ul>
                </li>
            {% endfor %}
        </ul>

    </section>


{% endblock body %}




